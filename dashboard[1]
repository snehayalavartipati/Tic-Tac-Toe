  <script type="text/javascript">
    
    window.addEventListener('load', (event) => {

        if(window.location.pathname == "/author/dashboard")
          document.getElementById('menu_dashboard').className = "font-weight-bold text-primary";

        if( (window.location.pathname == "/author/apps/create") || (window.location.pathname.includes("/edit")) )
          document.getElementById('menu_maker').className = "font-weight-bold text-primary";

        if(window.location.pathname == "/author/apps")
          document.getElementById('menu_apps').className = "font-weight-bold text-primary";

        if(window.location.pathname == "/author/stats")
          document.getElementById('menu_installs').className = "font-weight-bold text-primary";

        if(window.location.pathname == "/author/settings")
          document.getElementById('menu_settings').className = "font-weight-bold text-primary";

    });  

  </script>

